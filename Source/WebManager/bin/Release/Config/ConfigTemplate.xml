  <tab name="Compiler">

    <group name="Language features"  image="1">
      <check name="Short open tags" node="configuration/phpNet/compiler/set[@name='LanguageFeatures']">
        <add value="ShortOpenTags" phpName="short_open_tag" />
      </check>
      <check name="Allows ASP-style" node="configuration/phpNet/compiler/set[@name='LanguageFeatures']">
	      <add value="AspTags" phpName="asp_tags" />
	    </check>
	    <check name="Asp tags" node="configuration/phpNet/compiler/set[@name='LanguageFeatures']">
	      <add value="UnicodeSemantics" phpName="unicode.semantics" />
	    </check>
	    <check name="Enables PHP5 new keywords" node="configuration/phpNet/compiler/set[@name='LanguageFeatures']">
	      <add value="V5Keywords" />
	    </check>
	    <check name="Enables PHP6 new keywords" node="configuration/phpNet/compiler/set[@name='LanguageFeatures']">
	      <add value="V6Keywords" />
	    </check>
	    <check name="TypeKeywords" node="configuration/phpNet/compiler/set[@name='LanguageFeatures']">
	      <add value="TypeKeywords" />
	    </check>
	    <check name="ClrSemantics" node="configuration/phpNet/compiler/set[@name='LanguageFeatures']">
	      <add value="ClrSemantics" />
	    </check>
	    <check name="Asp tags" node="configuration/phpNet/compiler/set[@name='LanguageFeatures']">
	      <add value="Linq" />
	    </check>	

    </group>

    <group name="Compiler"  image="1">
      <dropdown name="Debug Mode"  node="configuration/phpNet/compiler/set[@name='Debug']/@value">
        <option name="Yes">true</option>
        <option name="No">false</option>
      </dropdown>
    </group>


    <group name="Static Inclusion"  image="1">

      <dropdown name="Static Inclusion" node="configuration/phpNet/compiler/set[@name='EnableStaticInclusions']/@value">
        <option name="Enabled">true</option>
        <option name="Disabled">false</option>
      </dropdown>

      <dropdown name="Static Inclusion Scope" node="configuration/phpNet/compiler/set[@name='EnableStaticInclusions']/@scope">
        <option name="Any">Any</option>
        <option name="Runtime">Runtime</option>
        <option name="Build">Build</option>
      </dropdown>

      <textbox name="Static Include Paths" node ="configuration/phpNet/compiler/set[@name='StaticIncludePaths']/@value">
        .
      </textbox>

    </group>

    <group name="InclusionMappings"  image="1">
      <check name="Named mapping"  node="configuration/phpNet/compiler/set[@name='StaticIncludePaths']">
        <add pattern="" value="" name="" />
      </check>
      <check name="Anonymous mapping"  node="configuration/phpNet/compiler/set[@name='StaticIncludePaths']">
        <add pattern="" value="" />
      </check>
      <check name="Remove named pattern"  node="configuration/phpNet/compiler/set[@name='StaticIncludePaths']">
        <remove name=""/>
      </check>
      <check name="Named mapping"  node="configuration/phpNet/compiler/set[@name='StaticIncludePaths']">
        <clear/>
      </check>
    </group>

    <group name="PrependFile"  image="1">
      <check name="PrependFile"  node="configuration/phpNet/compiler">
        <set name="PrependFile" value="" phpName="auto_prepend_file" />
      </check>

      <textbox name="PrependFileName" node ="configuration/phpNet/compiler/set[@name='PrependFile']/@value">
      </textbox>
    </group>

    <group name="AppendFile"  image="1">
      <check name="AppendFile"  node="configuration/phpNet/compiler">
        <set name="AppendFile" value="" phpName="auto_append_file" />
      </check>

      <textbox name="AppendFileName" node ="configuration/phpNet/compiler/set[@name='PrependFile']/@value">
      </textbox>
    </group>
    
  </tab>

</phpNet>



